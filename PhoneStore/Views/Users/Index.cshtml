@model IEnumerable<User>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h1>Список пользователей</h1>
<table class="table">
    <tr>
        <th>
            <a asp-action="Index" asp-route-state="@ViewBag.NameSort">
                Имя
            </a>
        </th>
        <th>
            <a asp-action="Index" asp-route-state="@ViewBag.AgeSort">
                Возраст
            </a>
        </th>
        <th>
            <a asp-action="Index"asp-route-state="@ViewBag.BrandSort">
                Компания
            </a>
        </th>
    </tr>

    @foreach (User u in Model)
    {
        <tr><td>@u.Name</td><td>@u.Age</td><td>@u.Brand.Name</td></tr>
    }
</table>
